docker run # 이미지 파일을 다운 받고 container 생성해서 start

docker pull # 이미지 파일만 다운 받습니다 - docker pull node - docker pull python
docker start # 이미지 파일로 container 생성해서 start
docker stop # container 멈춤

docker run -it -d -p 8000:8000 --name=nodejs* node
docker run -it -d -p 3000:3000 --name=nodejs* node

docker images # images list 보기

docker cp app.js node.js\_:/app.js
